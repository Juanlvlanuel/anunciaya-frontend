{
  "type": "module",
  "name": "anunciaya-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --host",
    "build": "cross-env ROLLUP_SKIP_NODEJS_NATIVE=1 vite build",
    "preview": "vite preview",
    "cap:use:dev": "node -e \"require('fs').copyFileSync('capacitor.config.dev.json','capacitor.config.json')\" && npx cap sync",
    "cap:use:prod": "node -e \"require('fs').copyFileSync('capacitor.config.prod.json','capacitor.config.json')\" && npx cap sync",
    "cap:run:dev": "npm run cap:use:dev && npm run dev",
    "cap:android:dev": "npm run cap:use:dev && npx cap run android -l --external",
    "cap:build:prod": "npm run cap:use:prod && npm run build && npx cap copy",
    "cap:apk:prod": "npm run cap:build:prod && echo Ahora genera el APK en Android Studio: Build > Build APK(s)"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.3",
    "@capacitor/cli": "^7.4.3",
    "@capacitor/core": "^7.4.3",
    "@capacitor/geolocation": "^7.1.5",
    "@capacitor/status-bar": "^7.0.2",
    "@capgo/capacitor-social-login": "^7.11.0",
    "@greatsumini/react-facebook-login": "^3.4.0",
    "@hello-pangea/dnd": "^18.0.1",
    "@heroicons/react": "^2.2.0",
    "@react-oauth/google": "^0.12.2",
    "axios": "^1.11.0",
    "emoji-picker-react": "^4.13.2",
    "framer-motion": "^12.23.12",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.525.0",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.5.0",
    "react-fast-marquee": "^1.6.5",
    "react-icons": "^5.5.0",
    "react-international-phone": "^4.6.0",
    "react-phone-input-2": "^2.15.1",
    "react-router-dom": "^7.7.0",
    "react-toastify": "^11.0.5",
    "socket.io-client": "^4.8.1",
    "sweetalert2": "^11.22.2",
    "swiper": "^11.2.10",
    "twemoji": "^14.0.2"
  },
  "devDependencies": {
    "@tailwindcss/line-clamp": "^0.4.4",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.0",
    "cross-env": "^10.0.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.3.0",
    "vite": "^7.0.6"
  }
}
